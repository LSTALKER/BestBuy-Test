<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical">
	<ImageView
		style="@style/drop_shadow" 
		android:id="@+id/shadow"
		android:contentDescription="@string/image_content_description"/>
	<LinearLayout
		android:orientation="vertical"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:background="#FFF5F5F5"
		android:id="@+id/star_rating_details">
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_marginLeft="10dip"
			android:layout_marginRight="10dip"
			android:layout_marginBottom="15dip"
			android:layout_marginTop="5dip"
			android:orientation="vertical"
			android:layout_height="wrap_content"
			android:layout_gravity="center"
			style="@style/commerce_box.white.noclick">
			<RelativeLayout
				android:id="@+id/product_review_category_one"
				android:layout_width="fill_parent"
				android:padding="6dip"
				android:layout_height="wrap_content"
				android:textColor="#333">
				<TextView
					android:id="@+id/product_review_category_one_title"
					android:padding="2dip"
					android:layout_alignParentLeft="true"
					android:textStyle="bold"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textColor="#FF003B64"
					android:textSize="14sp"
					/>
				<ImageView
					android:id="@+id/product_review_category_one_star_iv"
					android:src="@drawable/stars2"
					android:padding="2dip"
					android:layout_alignParentRight="true"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginTop="5dip" 
					android:contentDescription="@string/image_content_description"/>
			</RelativeLayout>
			<View
				android:id="@+id/product_review_star_rating_line_1"
				style="@style/horizontal_line" />
			<RelativeLayout
				android:id="@+id/product_review_category_two"
				android:layout_width="fill_parent"
				android:padding="6dip"
				android:layout_height="wrap_content"
				android:textColor="#333">
				<TextView
					android:id="@+id/product_review_category_two_title"
					android:padding="2dip"
					android:layout_alignParentLeft="true"
					android:textStyle="bold"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textColor="#FF003B64"
					android:textSize="14sp"
					/>
				<ImageView
					android:id="@+id/product_review_category_two_star_iv"
					android:src="@drawable/stars2"
					android:padding="2dip"
					android:layout_alignParentRight="true"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginTop="5dip" 
					android:contentDescription="@string/image_content_description"/>
			</RelativeLayout>
			<View
				android:id="@+id/product_review_star_rating_line_2"
				style="@style/horizontal_line" />
			<RelativeLayout
				android:id="@+id/product_review_category_three"
				android:layout_width="fill_parent"
				android:padding="6dip"
				android:layout_height="wrap_content"
				android:textColor="#333">
				<TextView
					android:id="@+id/product_review_category_three_title"
					android:padding="2dip"
					android:layout_alignParentLeft="true"
					android:textStyle="bold"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textColor="#FF003B64"
					android:textSize="14sp"
					/>
				<ImageView
					android:id="@+id/product_review_category_three_star_iv"
					android:src="@drawable/stars2"
					android:padding="2dip"
					android:layout_alignParentRight="true"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginTop="5dip"
					android:contentDescription="@string/image_content_description" />
			</RelativeLayout>
			<View
				android:id="@+id/product_review_star_rating_line_3"
				style="@style/horizontal_line" />
			<RelativeLayout
				android:id="@+id/product_review_category_four"
				android:layout_width="fill_parent"
				android:padding="6dip"
				android:layout_height="wrap_content"
				android:textColor="#333">
				<TextView
					android:id="@+id/product_review_category_four_title"
					android:textStyle="bold"
					android:padding="2dip"
					android:layout_alignParentLeft="true"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textColor="#FF003B64"
					android:textSize="14sp"
					/>
				<ImageView
					android:id="@+id/product_review_category_four_star_iv"
					android:src="@drawable/stars2"
					android:padding="2dip"
					android:layout_alignParentRight="true"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginTop="5dip" 
					android:contentDescription="@string/image_content_description"/>
			</RelativeLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>